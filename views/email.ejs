<body>
            <header>
                <nav class="navbar navbar-dark">
                    <a class="navbar-brand" href="/">RYDE</a>
                </nav>
        </header>
        <br><br>
        <div class="container marketing text-center">
        
            <div class="container text-center">
                <div style="background-color: white;"
                    class="row p-4 pb-0 pe-lg-0 pt-lg-5 align-items-center rounded-3 border shadow-lg justify-content-center">
                    <div class="row featurette text-center">
                        <div class="justify-content-center text-center">
                            
                            <h2 class="featurette-heading text-center">Your RYDE is<span class="text-muted"> Booked!</h2></span>
                            </h2>
                            <p class="lead text-center">Thank you for booking with RYDE! Here's your email confirmation with all the information required for contact.</p>
                            <!-- <a href="/"><button type="button" class="btn explore-btn text-center">
                                    <- Go home</button></a> -->
                        </div>
        
                    </div>
                </div>
            </div>
        </div>
        <br><br>
        <div class="container">
            <div class="row">
                <div style="background-color: white;" class="col-md-5 rounded-3 border shadow-lg ">
                    <br>
                    <div class="bg-light rounded d-flex flex-column">
                        <div class="p-2 ml-3">
                            <h4>Ryde Contact Information</h4>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">First Name</div>
                            <div class="ml-auto">
                                <%= user.Fname %>
                            </div>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">Last Name</div>
                            <div class="ml-auto">
                                <%= user.Lname %>
                            </div>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-7">Mobile</div>
                            <div class="ml-auto">
                                +91 <%= user.Mobile %>
                            </div>
                        </div>
                        <div class="border-top px-4 mx-3"> </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">City</div>
                            <div class="ml-auto">
                                <%= user.City %>
                            </div>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">State</div>
                            <div class="ml-auto">
                                <%= user.State %>
                            </div>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">Zip</div>
                            <div class="ml-auto">
                                <%= user.Zip %>
                            </div>
                        </div>
                    </div>
                    <br>
                </div>
                <div class="col-md-2"></div>
                <div style="background-color: white;" class="col-md-5 rounded-3 border shadow-lg ">
                    <br>
                    <div class="bg-light rounded d-flex flex-column">
                        <div class="p-2 ml-3">
                            <h4>Order Invoice</h4>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">Start Date</div>
                            <div class="ml-auto">
                                <%= String(details.Date).slice(0,10) %>
                            </div>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">Start Time</div>
                            <div class="ml-auto">
                                <%= String(details.Date).slice(11) %>
                            </div>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">End Date</div>
                            <div class="ml-auto">
                                <%= String(details.Return_date).slice(0,10) %>
                            </div>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">End Time</div>
                            <div class="ml-auto">
                                <%= String(details.Return_date).slice(11) %>
                            </div>
                        </div>
                        <div class="border-top px-4 mx-3"> </div>
                        <div class="p-2 d-flex pt-3">
                            <div class="col-8">Hourly Price</div>
                            <div class="ml-auto">₹<%= hourprice %>
                            </div>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">Hours</div>
                            <div class="ml-auto">
                                <%= details.Duration_hrs %> hrs
                            </div>
                        </div>
                        <div class="border-top px-4 mx-3"></div>
                        <div class="p-2 d-flex">
                            <div class="col-8">Base Price</div>
                            <div class="ml-auto">₹<%= baseprice %>
                            </div>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">Security Deposit
                            </div>
                            <div class="ml-auto">₹5000
                            </div>
                        </div>
                        <div class="p-2 d-flex">
                            <div class="col-8">GST( 18% )</div>
                            <div class="ml-auto">₹<%= Math.round(baseprice*0.18) %>
                            </div>
                        </div>
                        <div class="border-top px-4 mx-3"></div>
                        <div class="p-2 d-flex pt-3">
                            <div class="col-8"><b>Total</b></div>
                            <div class="ml-auto"><b class="green">₹<%= details.Price %></b></div>
                        </div>
                    </div>
                    <br>
                </div>
            </div>
        </div>
                <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
                    integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous">
                    </script>
                <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js"
                    integrity="sha384-Piv4xVNRyMGpqkS2by6br4gNJ7DXjqk09RmUpJ8jgGtD7zP9yug3goQfGII0yAns" crossorigin="anonymous">
                    </script>
</body>
</html>